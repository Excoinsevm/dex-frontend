{
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.sortImports": true
  },
  "editor.tabSize": 2,
  "explorerExclude.backup": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "yarn.lock": true,
    "README.md": true,
    "tsconfig.json": true,
    "package.json": true,
    "package-lock.json": true,
    "next.config.js": true,
    "next-env.d.ts": true,
    ".prettierignore": true,
    "cspell.json": true,
    ".prettierrc.json": true,
    ".npmrc": true,
    ".gitignore": true,
    ".eslintrc.cjs": true,
    ".eslintignore": true,
    "temp": true,
    "public": true,
    "node_modules": true,
    ".vscode": true,
    ".next": true,
    ".husky": true
  },
  "files.eol": "\r\n",
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "yarn.lock": false,
    "README.md": false,
    "tsconfig.json": false,
    "package.json": false,
    "package-lock.json": false,
    "next.config.js": false,
    "next-env.d.ts": false,
    ".prettierignore": false,
    "cspell.json": false,
    ".prettierrc.json": false,
    ".npmrc": false,
    ".gitignore": false,
    ".eslintrc.cjs": false,
    ".eslintignore": false,
    "temp": false,
    "public": false,
    "node_modules": false,
    ".vscode": false,
    ".next": false,
    ".husky": false
  },
  "prettier.configPath": ".prettierrc.json",
  "prettier.endOfLine": "crlf",
  "prettier.useTabs": false
}
